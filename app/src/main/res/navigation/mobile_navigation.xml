<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/navigation_home"
    >

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.crazy4web.myapplication.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.crazy4web.myapplication.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.crazy4web.myapplication.ui.notifications.NotificationsFragment"
        android:label="@string/title_favourites"
        tools:layout="@layout/fragment_notifications" />


    <fragment
        android:id="@+id/navigation_Chat"
        android:name="com.crazy4web.myapplication.ui.chat.ChatFragment"
        android:label="Chat"
        tools:layout="@layout/activity_chat_fragment" />

</navigation>